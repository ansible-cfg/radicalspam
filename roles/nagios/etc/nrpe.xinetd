# default: on
# description: NRPE (Nagios Remote Plugin Executor)
service nrpe
{
       	flags           = REUSE
        socket_type     = stream        
       	wait            = no
        user            = nagios
	group		= nagios
       	server          = /usr/sbin/nrpe
        server_args     = -n -c /var/rs/addons/nagios/etc/nrpe.cfg -i
       	log_on_failure  += USERID
        disable         = no
	only_from       = 91.121.40.27/32
}
